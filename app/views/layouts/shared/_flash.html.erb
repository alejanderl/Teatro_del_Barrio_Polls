
<div class="row">
	<% flash.each do |key, value| %>
		<div id="<%= "flash_#{key}" %>" class="flash  msg_box  <%= key %>">
		   <a
		    onclick="$('#flash_<%= key %>').fadeOut('slow')"
		    href="#"
		    title="<%= 'Close message'.t %>"
		    class="close"
		    >
		  &times;</a>
		  <%= value %>



		</div>
	<% end %>
</div>
